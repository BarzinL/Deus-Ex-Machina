      "description": "Standard confidence profile based on current physics understanding (2025)",
      "z_ranges": {
        "observed": {
          "description": "Experimentally observed elements with spectroscopic data",
          "range": [1, 118],
          "confidence": {
            "electron_configuration": 1.0,
            "atomic_radius": 0.95,
            "electronegativity": 0.95,
            "ionization_energy": 0.95,
            "oxidation_states": 0.90,
            "half_life": 0.90,
            "note": "Experimental spectroscopy and measurements"
          }
        },
        "synthesis_active": {
          "description": "Elements 119-120 with active synthesis attempts",
          "range": [119, 120],
          "confidence": {
            "electron_configuration": 0.85,
            "atomic_radius": 0.60,
            "electronegativity": 0.55,
            "ionization_energy": 0.60,
            "oxidation_states": 0.70,
            "half_life": 0.50,
            "note": "Multiple theoretical models agree, synthesis planned 2026-2028"
          }
        },
        "island_of_stability": {
