{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Confidence scoring profiles for element property predictions. These ranges can be adjusted as physics understanding improves without changing code.",
  "version": "1.0.0",
  "last_updated": "2025-11-23",

  "profiles": {
